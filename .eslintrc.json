{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "amd": true
  },
  "globals": {
    "wx": false,
    "test": true
  },
  "plugins": [
    "html"
  ],
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "globalReturn": true,
      "impliedStrict": true,
      "jsx": false
    }
  },
  "rules": {
    "indent": [1, 2],
    "no-console": 0,
    "no-debugger": 0,
    "no-unused-vars": 1,
    "linebreak-style": 0,
    "no-empty": 1,
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      1,
      "never"
    ]
  }
}
